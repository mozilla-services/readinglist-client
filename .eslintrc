parser: babel-eslint

env:
  browser: true
  node: true

rules:
  camelcase: 0
  comma-dangle: 0
  eol-last: 0
  key-spacing: 0
  no-alert: 0
  no-comma-dangle: 0
  no-mixed-requires: 0
  no-multi-spaces: 0
  no-new: 0
  no-shadow: 0
  no-trailing-spaces: 2
  no-underscore-dangle: 0
  no-unused-vars: 1
  quotes: [1, double]
  semi-spacing: 0
